    "def create_3d_trajectory_plot():\n",
    "    \"\"\"创建3D轨迹可视化\"\"\"\n",
    "    fig = go.Figure()\n",
    "    \n",
    "    # 时间范围\n",
    "    t_range = np.linspace(0, 30, 300)\n",
    "    \n",
    "    # 导弹轨迹\n",
    "    missile_traj = np.array([missile_position(t, M1_initial, missile_velocity) for t in t_range])\n",
    "    \n",
    "    fig.add_trace(go.Scatter3d(\n",
    "        x=missile_traj[:, 0],\n",
    "        y=missile_traj[:, 1], \n",
    "        z=missile_traj[:, 2],\n",
    "        mode='lines',\n",
    "        line=dict(color='red', width=8),\n",
    "        name='导弹M1轨迹',\n",
    "        hovertemplate='导弹M1<br>X: %{x:.0f}m<br>Y: %{y:.0f}m<br>Z: %{z:.0f}m<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    # 无人机轨迹\n",
    "    drone_traj = np.array([drone_position(t, FY1_initial, drone_velocity) for t in t_range if t <= 20])\n",
    "    \n",
    "    fig.add_trace(go.Scatter3d(\n",
    "        x=drone_traj[:, 0],\n",
    "        y=drone_traj[:, 1],\n",
    "        z=drone_traj[:, 2],\n",
    "        mode='lines',\n",
    "        line=dict(color='blue', width=6),\n",
    "        name='无人机FY1轨迹',\n",
    "        hovertemplate='无人机FY1<br>X: %{x:.0f}m<br>Y: %{y:.0f}m<br>Z: %{z:.0f}m<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    # 烟幕云团轨迹\n",
    "    cloud_times = np.linspace(t_explode, t_explode + effective_duration, 100)\n",
    "    cloud_traj = []\n",
    "    for t in cloud_times:\n",
    "        pos = smoke_cloud_position(t, t_explode, explode_position)\n",
    "        if pos is not None:\n",
    "            cloud_traj.append(pos)\n",
    "    \n",
    "    if cloud_traj:\n",
    "        cloud_traj = np.array(cloud_traj)\n",
    "        fig.add_trace(go.Scatter3d(\n",
    "            x=cloud_traj[:, 0],\n",
    "            y=cloud_traj[:, 1],\n",
    "            z=cloud_traj[:, 2],\n",
    "            mode='lines+markers',\n",
    "            line=dict(color='purple', width=4),\n",
    "            marker=dict(size=3, color='purple'),\n",
    "            name='烟幕云团轨迹',\n",
    "            hovertemplate='烟幕云团<br>X: %{x:.0f}m<br>Y: %{y:.0f}m<br>Z: %{z:.0f}m<extra></extra>'\n",
    "        ))\n",
    "    \n",
    "    # 关键点标记\n",
    "    fig.add_trace(go.Scatter3d(\n",
    "        x=[M1_initial[0]], y=[M1_initial[1]], z=[M1_initial[2]],\n",
    "        mode='markers',\n",
    "        marker=dict(size=15, color='red', symbol='diamond'),\n",
    "        name='M1初始位置',\n",
    "        hovertemplate='M1初始位置<br>X: %{x:.0f}m<br>Y: %{y:.0f}m<br>Z: %{z:.0f}m<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    fig.add_trace(go.Scatter3d(\n",
    "        x=[FY1_initial[0]], y=[FY1_initial[1]], z=[FY1_initial[2]],\n",
    "        mode='markers',\n",
    "        marker=dict(size=12, color='blue', symbol='square'),\n",
    "        name='FY1初始位置',\n",
    "        hovertemplate='FY1初始位置<br>X: %{x:.0f}m<br>Y: %{y:.0f}m<br>Z: %{z:.0f}m<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    fig.add_trace(go.Scatter3d(\n",
    "        x=[deploy_position[0]], y=[deploy_position[1]], z=[deploy_position[2]],\n",
    "        mode='markers',\n",
    "        marker=dict(size=10, color='orange', symbol='circle'),\n",
    "        name='投放点',\n",
    "        hovertemplate='投放点<br>X: %{x:.0f}m<br>Y: %{y:.0f}m<br>Z: %{z:.0f}m<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    fig.add_trace(go.Scatter3d(\n",
    "        x=[explode_position[0]], y=[explode_position[1]], z=[explode_position[2]],\n",
    "        mode='markers',\n",
    "        marker=dict(size=12, color='purple', symbol='star'),\n",
