    "        scene=dict(\n",
    "            xaxis_title='X坐标 (m)',\n",
    "            yaxis_title='Y坐标 (m)', \n",
    "            zaxis_title='Z坐标 (m)',\n",
    "            camera=dict(\n",
    "                eye=dict(x=1.5, y=1.5, z=1.2)\n",
    "            )\n",
    "        ),\n",
    "        width=1200,\n",
    "        height=800,\n",
    "        showlegend=True\n",
    "    )\n",
    "    \n",
    "    return fig\n",
    "\n",
