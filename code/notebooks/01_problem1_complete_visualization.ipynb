    "\n",
    "print(f\"\\nğŸ‰ è®¡ç®—ç»“æœ:\")\n",
    "print(f\"æœ‰æ•ˆé®è”½æ—¶é•¿: {shielding_duration:.3f} ç§’\")\n",
    "print(f\"é®è”½è®°å½•æ•°é‡: {len(records)}\")\n",
    "\n",
    "if records:\n",
    "    print(f\"é®è”½å¼€å§‹æ—¶é—´: {records[0]['time']:.3f}s\")\n",
